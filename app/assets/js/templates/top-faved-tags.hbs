<h2>Most faved tags</h2>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Tag</th>
        <th>Count</th>
    </tr>
    </thead>
    <tbody>
    {{#each tags}}
        <tr>
            <td>{{tag}}</td>
            <td>{{count}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>