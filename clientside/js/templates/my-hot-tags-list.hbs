<ul>

    {{#each items}}
        <li>
            <strong>{{tag}}</strong> ({{count}} / {{f avg_points}} / {{f top_avg_points}})
        </li>
        <li>
            {{#each topphotos}}
                <a href="https://www.flickr.com/photos/{{owner}}/{{id}}" target="_blank" title="{{attr title}} ({{count_faves}})"><img src="{{urls.largeSquareThumb}}"></a>
            {{/each}}

        </li>
    {{/each}}

</ul>